<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisLanguage" table="SymbiosisLanguage" schema=""
           catalog="symbiosis">
        <id name="symbiosisLanguageId">
            <column name="SymbiosisLanguageID" sql-type="int" not-null="true"/>
        </id>
        <property name="symbiosisLanguageName">
            <column name="SymbiosisLanguageName" sql-type="varchar" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>