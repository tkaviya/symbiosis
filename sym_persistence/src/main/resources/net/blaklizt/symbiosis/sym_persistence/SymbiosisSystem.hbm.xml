<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisSystem" table="SymbiosisSystem" schema=""
           catalog="symbiosis">
        <id name="symbiosisSystemId">
            <column name="SymbiosisSystemID" sql-type="int" not-null="true"/>
        </id>
        <property name="description">
            <column name="Description" sql-type="varchar" length="128" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>