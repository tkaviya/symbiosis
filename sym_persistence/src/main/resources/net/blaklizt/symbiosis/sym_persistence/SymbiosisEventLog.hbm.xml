<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisEventLog" table="SymbiosisEventLog" schema=""
           catalog="symbiosis">
        <id name="symbiosisEventLogId">
            <column name="SymbiosisEventLogID" sql-type="bigint" not-null="true"/>
        </id>
        <property name="symbiosisEventTypeId">
            <column name="SymbiosisEventTypeID" sql-type="bigint"/>
        </property>
        <property name="eventDate">
            <column name="EventDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="symbiosisUserId">
            <column name="SymbiosisUserID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="description">
            <column name="Description" sql-type="varchar" length="256" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>