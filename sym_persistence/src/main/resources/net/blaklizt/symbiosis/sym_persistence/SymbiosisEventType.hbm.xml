<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisEventType" table="SymbiosisEventType" schema=""
           catalog="symbiosis">
        <id name="symbiosisEventTypeId">
            <column name="SymbiosisEventTypeID" sql-type="bigint" not-null="true"/>
        </id>
        <property name="description">
            <column name="Description" sql-type="varchar" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>