<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisUser" table="SymbiosisUser" schema=""
           catalog="symbiosis">
        <id name="symbiosisUserId">
            <column name="SymbiosisUserID" sql-type="bigint" not-null="true"/>
        </id>
        <property name="firstName">
            <column name="FirstName" sql-type="varchar" length="50"/>
        </property>
        <property name="lastName">
            <column name="LastName" sql-type="varchar" length="50"/>
        </property>
        <property name="username">
            <column name="Username" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="email">
            <column name="Email" sql-type="varchar" length="50"/>
        </property>
        <property name="msisdn">
            <column name="Msisdn" sql-type="varchar" length="15"/>
        </property>
        <property name="password">
            <column name="Password" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="salt">
            <column name="Salt" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="symbiosisUserGroupId">
            <column name="SymbiosisUserGroupID" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="symbiosisChannelId">
            <column name="SymbiosisChannelID" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="symbiosisUserStatusId">
            <column name="SymbiosisUserStatusID" sql-type="int" not-null="true"/>
        </property>
        <property name="deviceId">
            <column name="DeviceID" sql-type="varchar" length="50"/>
        </property>
        <property name="accessSystemId">
            <column name="AccessSystemID" sql-type="varchar" length="50"/>
        </property>
        <property name="symbiosisCountryId">
            <column name="SymbiosisCountryID" sql-type="int"/>
        </property>
        <property name="symbiosisLanguageId">
            <column name="SymbiosisLanguageID" sql-type="int"/>
        </property>
        <property name="authToken">
            <column name="AuthToken" sql-type="varchar" length="256"/>
        </property>
        <property name="registrationDate">
            <column name="RegistrationDate" sql-type="datetime"/>
        </property>
        <property name="lastAuthDate">
            <column name="LastAuthDate" sql-type="datetime"/>
        </property>
        <property name="lastLoginDate">
            <column name="LastLoginDate" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>