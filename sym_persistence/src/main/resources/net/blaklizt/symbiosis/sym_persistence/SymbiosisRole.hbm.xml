<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisRole" table="SymbiosisRole" schema=""
           catalog="symbiosis">
        <id name="symbiosisRoleId">
            <column name="SymbiosisRoleID" sql-type="int" not-null="true"/>
        </id>
        <property name="description">
            <column name="Description" sql-type="varchar" length="20"/>
        </property>
        <property name="enabled">
            <column name="Enabled" sql-type="tinyint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>