<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.symbiosis.sym_persistence.SymbiosisCountry" table="SymbiosisCountry" schema=""
           catalog="symbiosis">
        <id name="symbiosisCountryId">
            <column name="SymbiosisCountryID" sql-type="int" not-null="true"/>
        </id>
        <property name="countryName">
            <column name="CountryName" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="countryCodePrefix">
            <column name="CountryCodePrefix" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>